{
  "name": "@examples/svelte-kit-cloudflare",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "cf:preview": "wrangler dev",
    "cf:deploy": "wrangler deploy",
    "cf:build": "wrangler deploy --dry-run --outdir=.workers-build"
  },
  "dependencies": {
    "@cf-wasm/og": "workspace:*",
    "@cf-wasm/photon": "workspace:*",
    "@cf-wasm/plugins": "workspace:*",
    "@cf-wasm/resvg": "workspace:*",
    "@cf-wasm/satori": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^24.10.1",
    "svelte": "^5.43.6",
    "svelte-check": "^4.3.4",
    "vite": "catalog:default",
    "vite-plugin-devtools-json": "^1.0.0",
    "wrangler": "catalog:default"
  }
}
