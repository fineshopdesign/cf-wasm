{
  "name": "@cf-wasm/resvg",
  "version": "0.2.3",
  "private": false,
  "description": "A high-performance SVG renderer and toolkit, powered by Rust based resvg.",
  "keywords": [
    "resvg",
    "svg",
    "resvg-js",
    "resvg-wasm",
    "rust",
    "svg2png",
    "svg2img",
    "svg to png",
    "wasm",
    "cloudflare-workers"
  ],
  "license": "MPL-2.0",
  "author": {
    "name": "yisibl"
  },
  "homepage": "https://github.com/fineshopdesign/cf-wasm/tree/main/packages/resvg#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/fineshopdesign/cf-wasm",
    "directory": "packages/resvg"
  },
  "bugs": {
    "url": "https://github.com/fineshopdesign/cf-wasm/issues"
  },
  "scripts": {
    "lint": "biome lint ./src",
    "check": "biome check ./src",
    "check:types": "tsc --noEmit",
    "build": "NODE_ENV=production tsup",
    "dev": "NODE_ENV=development tsup --watch"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/workerd.js",
  "types": "./dist/workerd.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./resvg.wasm": "./dist/lib/resvg.wasm",
    "./legacy/resvg.wasm": "./dist/legacy/lib/resvg.wasm",
    ".": {
      "node": {
        "import": {
          "types": "./dist/node.d.ts",
          "default": "./dist/node.js"
        },
        "require": {
          "types": "./dist/node.d.cts",
          "default": "./dist/node.cjs"
        }
      },
      "edge-light": {
        "types": "./dist/next.d.ts",
        "import": "./dist/next.js"
      },
      "workerd": {
        "types": "./dist/workerd.d.ts",
        "import": "./dist/workerd.js"
      },
      "default": {
        "types": "./dist/workerd.d.ts",
        "import": "./dist/workerd.js"
      }
    },
    "./workerd": {
      "types": "./dist/workerd.d.ts",
      "import": "./dist/workerd.js"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.js"
    },
    "./node": {
      "import": {
        "types": "./dist/node.d.ts",
        "default": "./dist/node.js"
      },
      "require": {
        "types": "./dist/node.d.cts",
        "default": "./dist/node.cjs"
      }
    },
    "./others": {
      "import": {
        "types": "./dist/others.d.ts",
        "default": "./dist/others.js"
      },
      "require": {
        "types": "./dist/others.d.cts",
        "default": "./dist/others.cjs"
      }
    },
    "./legacy": {
      "node": {
        "import": {
          "types": "./dist/legacy/node.d.ts",
          "default": "./dist/legacy/node.js"
        },
        "require": {
          "types": "./dist/legacy/node.d.cts",
          "default": "./dist/legacy/node.cjs"
        }
      },
      "edge-light": {
        "types": "./dist/legacy/next.d.ts",
        "import": "./dist/legacy/next.js"
      },
      "workerd": {
        "types": "./dist/legacy/workerd.d.ts",
        "import": "./dist/legacy/workerd.js"
      },
      "default": {
        "types": "./dist/legacy/workerd.d.ts",
        "import": "./dist/legacy/workerd.js"
      }
    },
    "./legacy/workerd": {
      "types": "./dist/legacy/workerd.d.ts",
      "import": "./dist/legacy/workerd.js"
    },
    "./legacy/next": {
      "types": "./dist/legacy/next.d.ts",
      "import": "./dist/legacy/next.js"
    },
    "./legacy/node": {
      "import": {
        "types": "./dist/legacy/node.d.ts",
        "default": "./dist/legacy/node.js"
      },
      "require": {
        "types": "./dist/legacy/node.d.cts",
        "default": "./dist/legacy/node.cjs"
      }
    },
    "./legacy/others": {
      "import": {
        "types": "./dist/legacy/others.d.ts",
        "default": "./dist/legacy/others.js"
      },
      "require": {
        "types": "./dist/legacy/others.d.cts",
        "default": "./dist/legacy/others.cjs"
      }
    }
  },
  "dependencies": {
    "@resvg/resvg-wasm": "^2.6.2",
    "@resvg/resvg-wasm-legacy": "npm:@resvg/resvg-wasm@2.4.1"
  },
  "devDependencies": {
    "@types/node": "catalog:default",
    "glob": "catalog:default",
    "tsup": "catalog:default"
  }
}
