{
  "name": "@cf-wasm/internals",
  "version": "0.0.3",
  "private": false,
  "description": "Internals for cf-wasm",
  "keywords": [],
  "license": "MIT",
  "author": {
    "name": "Deo Kumar",
    "url": "https://github.com/kumardeo"
  },
  "homepage": "https://github.com/fineshopdesign/cf-wasm/tree/main/packages/internals#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/fineshopdesign/cf-wasm",
    "directory": "packages/internals"
  },
  "bugs": {
    "url": "https://github.com/fineshopdesign/cf-wasm/issues"
  },
  "scripts": {
    "lint": "biome lint ./src",
    "check": "biome check ./src",
    "check:types": "tsc --noEmit",
    "build": "NODE_ENV=production tsup",
    "dev": "NODE_ENV=development tsup --watch"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./detect-runtime": {
      "import": {
        "types": "./dist/detect-runtime.d.ts",
        "default": "./dist/detect-runtime.js"
      },
      "require": {
        "types": "./dist/detect-runtime.d.cts",
        "default": "./dist/detect-runtime.cjs"
      }
    },
    "./polyfills/image-data": {
      "import": {
        "types": "./dist/polyfills/image-data.d.ts",
        "default": "./dist/polyfills/image-data.js"
      },
      "require": {
        "types": "./dist/polyfills/image-data.d.cts",
        "default": "./dist/polyfills/image-data.cjs"
      }
    }
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "tsup": "^8.5.0"
  }
}
