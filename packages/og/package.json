{
	"name": "@cf-wasm/og",
	"version": "0.1.13",
	"private": false,
	"description": "Generate Open Graph Images dynamically from HTML/CSS without a browser.",
	"keywords": [
		"open-graph-image",
		"open-graph",
		"og-image",
		"og:image",
		"social",
		"card",
		"image",
		"wasm",
		"cloudflare-workers"
	],
	"license": "MPL-2.0",
	"homepage": "https://github.com/fineshopdesign/cf-wasm/tree/main/packages/og#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/fineshopdesign/cf-wasm",
		"directory": "packages/og"
	},
	"bugs": {
		"url": "https://github.com/fineshopdesign/cf-wasm/issues"
	},
	"scripts": {
		"lint": "cross-env DEBUG=eslint:cli-engine eslint --fix --ignore-path ../../.eslintignore .",
		"format": "prettier --write --ignore-path ../../.prettierignore .",
		"build": "node scripts/build.mjs",
		"test": "vitest run",
		"clean": "rimraf dist"
	},
	"files": [
		"dist"
	],
	"main": "./dist/esm/workers/index.js",
	"types": "./dist/esm/workers/index.d.ts",
	"exports": {
		".": {
			"default": {
				"import": "./dist/esm/workers/index.js",
				"types": "./dist/esm/workers/index.d.ts",
				"default": "./dist/esm/workers/index.js"
			}
		},
		"./next": {
			"default": {
				"import": "./dist/esm/next/index.js",
				"types": "./dist/esm/next/index.d.ts"
			}
		},
		"./node": {
			"default": {
				"require": "./dist/cjs/node/index.js",
				"import": "./dist/cjs/node/index.js",
				"types": "./dist/cjs/node/index.d.ts"
			}
		},
		"./workers": {
			"default": {
				"import": "./dist/esm/workers/index.js",
				"types": "./dist/esm/workers/index.d.ts",
				"default": "./dist/esm/workers/index.js"
			}
		}
	},
	"typesVersions": {
		"*": {
			"next": [
				"./dist/esm/next/index.d.ts"
			],
			"node": [
				"./dist/cjs/node/index.d.ts"
			],
			"workers": [
				"./dist/esm/workers/index.d.ts"
			],
			"*": [
				"./dist/esm/workers/index.d.ts"
			]
		}
	},
	"dependencies": {
		"@cf-wasm/resvg": "workspace:*",
		"@cf-wasm/satori": "workspace:*"
	},
	"devDependencies": {
		"react": "^18.2.0"
	}
}
