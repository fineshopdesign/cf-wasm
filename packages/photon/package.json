{
	"name": "@cf-wasm/photon",
	"version": "0.1.6",
	"description": "Photon library for Cloudflare workers",
	"license": "Apache-2.0",
	"collaborators": [
		"Silvia O'Dwyer <silviaodwyerdev@gmail.com>"
	],
	"repository": "fineshopdesign/cf-wasm",
	"bugs": "https://github.com/fineshopdesign/cf-wasm/issues",
	"homepage": "https://silvia-odwyer.github.io/photon/",
	"files": [
		"dist/"
	],
	"exports": {
		"./next": {
			"default": {
				"import": "./dist/edge/next/index.js",
				"types": "./dist/edge/next/index.d.js"
			}
		},
		"./node": {
			"default": {
				"require": "./dist/node/cjs/index.js",
				"import": "./dist/node/cjs/index.js",
				"types": "./dist/node/cjs/index.d.js"
			}
		},
		"./workers": {
			"default": {
				"import": "./dist/edge/workers/index.js",
				"types": "./dist/edge/workers/index.d.js"
			}
		},
		".": {
			"default": {
				"import": "./dist/edge/workers/index.js",
				"types": "./dist/edge/workers/index.d.js",
				"default": "./dist/edge/workers/index.js"
			}
		}
	},
	"typesVersions": {
		"*": {
			"next": [
				"./dist/edge/next/index.d.js"
			],
			"node": [
				"./dist/node/cjs/index.d.js"
			],
			"workers": [
				"./dist/edge/workers/index.d.js"
			],
			"*": [
				"./dist/edge/workers/index.d.js"
			]
		}
	},
	"scripts": {
		"remove:files": "rm -f src/lib/package.json src/lib/LICENSE src/lib/LICENSE.md src/lib/.gitignore",
		"build:wasm": "wasm-pack build ./crate --out-dir ../src/lib --target web && npm run remove:files",
		"build": "tsc && tsc -p ./tsconfig.cjs.json",
		"postbuild": "copyfiles -u 1 ./src/lib/*.wasm ./dist/edge/ && copyfiles -u 1 ./src/lib/*.wasm ./dist/node/"
	}
}
