{
	"name": "@cf-wasm/png",
	"version": "0.1.5",
	"description": "A simple wasm png encoder/decoder module for cloudflare workers",
	"license": "MIT",
	"files": [
		"dist/",
		"lib/",
		"typings/**/*.d.ts",
		"typings/**/*.d.ts.map"
	],
	"repository": "fineshopdesign/cf-wasm",
	"bugs": "https://github.com/fineshopdesign/cf-wasm/issues",
	"homepage": "https://github.com/fineshopdesign/cf-wasm/tree/main/packages/png",
	"exports": {
		"./next": {
			"default": {
				"import": "./dist/next/index.js",
				"types": "./typings/next/index.d.ts"
			}
		},
		"./node": {
			"default": {
				"import": "./dist/cjs/index.js",
				"types": "./typings/cjs/index.d.ts"
			}
		},
		"./workers": {
			"default": {
				"import": "./dist/workers/index.js",
				"types": "./typings/workers/index.d.ts"
			}
		},
		".": {
			"default": {
				"types": "./dist/workers/index.d.ts",
				"default": "./dist/workers/index.js"
			}
		}
	},
	"typesVersions": {
		"*": {
			"next": [
				"./typings/next/index.d.ts"
			],
			"node": [
				"./typings/cjs/index.d.ts"
			],
			"workers": [
				"./typings/workers/index.d.ts"
			],
			"*": [
				"./typings/workers/index.d.ts"
			]
		}
	},
	"scripts": {
		"remove:files": "rm -f lib/package.json lib/LICENSE lib/LICENSE.md lib/.gitignore",
		"build:wasm": "wasm-pack build ./crate --out-dir ../lib --target web && npm run remove:files",
		"build": "tsc && tsc -p ./tsconfig.cjs.json"
	}
}
