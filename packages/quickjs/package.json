{
  "name": "@cf-wasm/quickjs",
  "version": "0.0.6",
  "private": false,
  "description": "QuickJS",
  "keywords": [],
  "license": "MIT",
  "homepage": "https://github.com/fineshopdesign/cf-wasm/tree/main/packages/quickjs#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/fineshopdesign/cf-wasm",
    "directory": "packages/quickjs"
  },
  "bugs": {
    "url": "https://github.com/fineshopdesign/cf-wasm/issues"
  },
  "scripts": {
    "lint": "biome lint ./src",
    "check": "biome check ./src",
    "check:types": "tsc --noEmit",
    "build": "NODE_ENV=production tsup",
    "dev": "NODE_ENV=development tsup --watch"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/workerd.js",
  "types": "./dist/workerd.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./DEBUG_SYNC.wasm": "./dist/core/DEBUG_SYNC.wasm",
    "./DEBUG_SYNC.wasm.map.txt": "./dist/core/DEBUG_SYNC.wasm.map.txt",
    "./RELEASE_SYNC.wasm": "./dist/core/RELEASE_SYNC.wasm",
    ".": {
      "types": "./dist/workerd.d.ts",
      "import": "./dist/workerd.js"
    },
    "./workerd": {
      "types": "./dist/workerd.d.ts",
      "import": "./dist/workerd.js"
    },
    "./workerd-debug": {
      "types": "./dist/workerd-debug.d.ts",
      "import": "./dist/workerd-debug.js"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.js"
    },
    "./next-debug": {
      "types": "./dist/next-debug.d.ts",
      "import": "./dist/next-debug.js"
    },
    "./node": {
      "import": {
        "types": "./dist/node.d.ts",
        "default": "./dist/node.js"
      },
      "require": {
        "types": "./dist/node.d.cts",
        "default": "./dist/node.cjs"
      }
    },
    "./node-debug": {
      "import": {
        "types": "./dist/node-debug.d.ts",
        "default": "./dist/node-debug.js"
      },
      "require": {
        "types": "./dist/node-debug.d.cts",
        "default": "./dist/node-debug.cjs"
      }
    }
  },
  "dependencies": {
    "@jitl/quickjs-wasmfile-debug-sync": "^0.31.0",
    "@jitl/quickjs-wasmfile-release-sync": "^0.31.0",
    "quickjs-emscripten-core": "0.31.0",
    "tsup": "^8.5.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "glob": "^11.0.3"
  }
}
