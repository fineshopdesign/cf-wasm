{
  "name": "@cf-wasm/plugins",
  "version": "0.1.7",
  "private": false,
  "description": "Plugins for cf-wasm",
  "keywords": [],
  "license": "MIT",
  "author": {
    "name": "Deo Kumar",
    "url": "https://github.com/kumardeo"
  },
  "homepage": "https://github.com/fineshopdesign/cf-wasm/tree/main/packages/plugins#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/fineshopdesign/cf-wasm",
    "directory": "packages/plugins"
  },
  "bugs": {
    "url": "https://github.com/fineshopdesign/cf-wasm/issues"
  },
  "scripts": {
    "lint": "biome lint ./src",
    "check": "biome check ./src",
    "check:types": "tsc --noEmit",
    "build": "NODE_ENV=production tsup",
    "dev": "NODE_ENV=development tsup --watch"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./nitro-additional-modules": {
      "import": {
        "types": "./dist/nitro-additional-modules.d.ts",
        "default": "./dist/nitro-additional-modules.js"
      },
      "require": {
        "types": "./dist/nitro-additional-modules.d.cts",
        "default": "./dist/nitro-additional-modules.cjs"
      }
    },
    "./rollup-additional-modules": {
      "import": {
        "types": "./dist/rollup-additional-modules.d.ts",
        "default": "./dist/rollup-additional-modules.js"
      },
      "require": {
        "types": "./dist/rollup-additional-modules.d.cts",
        "default": "./dist/rollup-additional-modules.cjs"
      }
    },
    "./vite-additional-modules": {
      "import": {
        "types": "./dist/vite-additional-modules.d.ts",
        "default": "./dist/vite-additional-modules.js"
      },
      "require": {
        "types": "./dist/vite-additional-modules.d.cts",
        "default": "./dist/vite-additional-modules.cjs"
      }
    }
  },
  "dependencies": {
    "magic-string": "^0.30.21"
  },
  "devDependencies": {
    "@types/node": "catalog:default",
    "nitro": "3.0.1-alpha.1",
    "rollup": "^4.53.3",
    "tsup": "catalog:default",
    "vite": "catalog:default"
  },
  "peerDependencies": {
    "rollup": "^4.0.0",
    "vite": "^6.1.0 || ^7.0.0"
  },
  "peerDependenciesMeta": {
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  }
}
