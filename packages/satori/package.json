{
  "name": "@cf-wasm/satori",
  "version": "0.2.1",
  "private": false,
  "description": "Enlightened library to convert HTML and CSS to SVG.",
  "keywords": [
    "HTML",
    "JSX",
    "SVG",
    "converter",
    "renderer",
    "wasm",
    "cloudflare-workers"
  ],
  "license": "MPL-2.0",
  "author": {
    "name": "Shu Ding",
    "email": "g@shud.in"
  },
  "homepage": "https://github.com/fineshopdesign/cf-wasm/tree/main/packages/satori#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/fineshopdesign/cf-wasm",
    "directory": "packages/satori"
  },
  "bugs": {
    "url": "https://github.com/fineshopdesign/cf-wasm/issues"
  },
  "scripts": {
    "lint": "biome lint ./src",
    "check": "biome check ./src",
    "check:types": "tsc --noEmit",
    "build": "NODE_ENV=production tsup",
    "dev": "NODE_ENV=development tsup --watch"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/workerd.js",
  "types": "./dist/workerd.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./yoga.wasm": "./dist/lib/yoga.wasm",
    ".": {
      "types": "./dist/workerd.d.ts",
      "import": "./dist/workerd.js"
    },
    "./workerd": {
      "types": "./dist/workerd.d.ts",
      "import": "./dist/workerd.js"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.js"
    },
    "./node": {
      "import": {
        "types": "./dist/node.d.ts",
        "default": "./dist/node.js"
      },
      "require": {
        "types": "./dist/node.d.cts",
        "default": "./dist/node.cjs"
      }
    },
    "./others": {
      "import": {
        "types": "./dist/others.d.ts",
        "default": "./dist/others.js"
      },
      "require": {
        "types": "./dist/others.d.cts",
        "default": "./dist/others.cjs"
      }
    }
  },
  "dependencies": {
    "@types/react": "^19.2.2",
    "react": "^19.2.0",
    "satori": "^0.15.2",
    "yoga-wasm-web": "^0.3.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "glob": "^11.0.3",
    "tsup": "^8.5.0"
  }
}
